// New Tilesets w/ palettes + anims

	// Desert Bastion
	ALIGN 4
	DesertBastionMapchipConfig:
	#incext Compress "Graphics/DesertBastion.mapchip_config"

	ALIGN 4
	DesertBastionPalette:
	#incext Png2Dmp "Graphics/DesertBastion.png"

	ALIGN 4
	DesertBastionObject:
	#incext Png2Dmp "Map/DesertBastion.png"
	
// PLIST

	EventPointerTable(CastleBlueFloor,CastleBlueFloorPalette)
	EventPointerTable(CastleGreenGold,CastleGreenGoldPalette)
	EventPointerTable(CastleNight,CastleNightPalette)
	EventPointerTable(FieldsNight,FieldsNightPalette)
	EventPointerTable(FieldsSummerTeal,FieldsSummerTealPalette)
	EventPointerTable(FieldsGreyMt,FieldsGreyMtPalette)
	EventPointerTable(FortFE7,FortFE7Palette)
	EventPointerTable(FortTeal,FortTealPalette)
	EventPointerTable(VillageFadedDay,VillageFadedDayPalette)
	EventPointerTable(VillageFE7,VillageFE7Palette)
	EventPointerTable(VillageNight,VillageNightPalette)
	
	