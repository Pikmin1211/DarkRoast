	CastleGreenGoldPalette:
	#incext Png2Dmp "Graphics/castle_palette_greengold.png" --palette-only
	
	ALIGN 4
	CastleNightPalette:
	#incext Png2Dmp "Graphics/castle_palette_nightfe7.png" --palette-only

	// Fields
	ALIGN 4
	FieldsNightPalette:
